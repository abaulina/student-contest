{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 2020,
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"react/no-unescaped-entities": 0,
		"strict": 0,
		"react/no-unknown-property": 0,
		"no-control-regex": 0
	}
}
