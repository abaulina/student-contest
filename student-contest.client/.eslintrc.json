{
	"env": {
		"browser": true,
		"es2021": true,
		"jest": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended",
		"plugin:testcafe/recommended"
	],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 2020,
		"sourceType": "module"
	},
	"plugins": ["react", "testcafe"],
	"rules": {
		"react/no-unescaped-entities": 0,
		"strict": 0,
		"react/no-unknown-property": 0,
		"no-control-regex": 0
	}
}
